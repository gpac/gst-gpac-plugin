  "version": "0.2.0",
  "configurations": [
    {
      "name": "AACLC",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/gst-launch",
      "preLaunchTask": "build",
      "postDebugTask": "graph",
      "args": [
        "-v",
        "audiotestsrc",
        "num-buffers=30",
        "!",
        "avenc_aac",
        "!",
        "identity",
        "silent=false",
        "!",
        "gpacsink",
        "graph=-o test.mp4"
      ],
      "cwd": "/tmp",
      "environment": [
        {
          "name": "GST_DEBUG",
          "value": "3"
        },
        {
          "name": "GST_PLUGIN_PATH",
          "value": "${workspaceFolder}/build"
        },
        {
          "name": "GST_DEBUG_DUMP_DOT_DIR",
          "value": "${workspaceFolder}/graph"
        }
      ],
      "osx": {
        "MIMode": "lldb",
        "targetArchitecture": "arm64",
        "internalConsoleOptions": "openOnSessionStart"
      }
    },
